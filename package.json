{
  "name": "fluentis-erp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& set HTTPS=false&& docusaurus start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus build",
    "swizzle": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus swizzle",
    "deploy": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus deploy",
    "clear": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus clear",
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus serve",
    "write-translations": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus write-translations",
    "write-heading-ids": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=8192&& docusaurus write-heading-ids"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^1.2.0",
    "@docusaurus/core": "^3.6.3",
    "@docusaurus/preset-classic": "^3.6.3",
    "@mdx-js/react": "^3.0.0",
    "bootstrap-icons": "^1.11.3",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.3",
    "@docusaurus/types": "^3.6.3"
  },
  "overrides": {
    "@cmfcmf/docusaurus-search-local": {
      "@docusaurus/core": "^3.6.3"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
